<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.ashu.ocotopus.ui.home.view.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_favorite"
        android:name="com.ashu.ocotopus.ui.favorite.FavoriteFragment"
        android:label="@string/favorite"
        tools:layout="@layout/fragment_favorite" />

    <fragment
        android:id="@+id/navigation_hungry"
        android:name="com.ashu.ocotopus.ui.hungry.HungryFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_hungry" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.ashu.ocotopus.ui.profile.ProfileFragment"
        android:label="@string/profile"
        tools:layout="@layout/fragment_hungry" />
</navigation>